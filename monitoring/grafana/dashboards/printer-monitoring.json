{
  "dashboard": {
    "id": null,
    "title": "Printernizer - Printer Monitoring Dashboard",
    "description": "Real-time monitoring of Bambu Lab A1 and Prusa Core One printers",
    "tags": ["printernizer", "3d-printing", "printers", "real-time"],
    "style": "dark",
    "timezone": "Europe/Berlin",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Printer Status Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "count(printernizer_printer_connected == 1)",
            "legendFormat": "Connected Printers",
            "refId": "A"
          },
          {
            "expr": "count(printernizer_printer_status == 1)",
            "legendFormat": "Active Jobs",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "green",
                  "value": 2
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Printer Temperatures",
        "type": "timeseries",
        "targets": [
          {
            "expr": "printernizer_printer_temperature{type=\"hotend\"}",
            "legendFormat": "{{printer_name}} Hotend",
            "refId": "A"
          },
          {
            "expr": "printernizer_printer_temperature{type=\"bed\"}",
            "legendFormat": "{{printer_name}} Bed",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "celsius",
            "min": 0,
            "max": 300,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 200
                },
                {
                  "color": "red",
                  "value": 250
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 16,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Print Job Progress",
        "type": "bargauge",
        "targets": [
          {
            "expr": "printernizer_print_progress",
            "legendFormat": "{{printer_name}} - {{job_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 25
                },
                {
                  "color": "green",
                  "value": 75
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 4,
        "title": "File Download Status",
        "type": "table",
        "targets": [
          {
            "expr": "printernizer_file_download_status",
            "format": "table",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "width": 120
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 5,
        "title": "WebSocket Connections",
        "type": "timeseries",
        "targets": [
          {
            "expr": "printernizer_websocket_connections",
            "legendFormat": "Active Connections",
            "refId": "A"
          },
          {
            "expr": "rate(printernizer_websocket_messages_total[5m])",
            "legendFormat": "Messages/sec",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 6,
        "title": "Network Connectivity",
        "type": "stat",
        "targets": [
          {
            "expr": "printernizer_printer_connectivity",
            "legendFormat": "{{printer_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "Disconnected"
                  },
                  "1": {
                    "text": "Connected"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 7,
        "title": "Business Metrics (German VAT)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "printernizer_business_revenue_eur",
            "legendFormat": "Revenue (EUR)",
            "refId": "A"
          },
          {
            "expr": "printernizer_business_vat_collected_eur",
            "legendFormat": "VAT Collected (19%)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "currencyEUR"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 20
        }
      }
    ]
  },
  "overwrite": true
}