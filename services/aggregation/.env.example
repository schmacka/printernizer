# Aggregation Service Configuration

# Database
# For SQLite (development):
DATABASE_URL=sqlite:///./aggregation.db
# For PostgreSQL (production):
# DATABASE_URL=postgresql://user:password@localhost:5432/printernizer_stats

# Authentication
API_KEY=change-me-in-production-to-secure-random-key

# Rate Limiting
RATE_LIMIT_PER_HOUR=10

# Logging
LOG_LEVEL=INFO

# Server
HOST=0.0.0.0
PORT=8080

# CORS (comma-separated list)
ALLOWED_ORIGINS=*
